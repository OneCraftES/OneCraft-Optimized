{
  "world_generator": {
    "generate_in_unloaded_chunks": true,
    "generate_fake_chunks": true
  },
  "rendering": {
    "render_distance": {
      "min_render_distance": 12,
      "max_render_distance": 32,
      "target_fps": 60
    },
    "lod_style": "HEIGHTMAP",
    "enable_cave_culling": true,
    "enable_fog": true,
    "fog_quality": "HIGH",
    "terrain_detail": "HIGH",
    "multithreading": {
      "enable_multithreaded_generation": true,
      "thread_count": "AUTO"
    }
  },
  "performance": {
    "max_generation_threads": 4,
    "buffer_cache_size_mb": 512,
    "enable_memory_management": true,
    "enable_aggressive_culling": true
  },
  "debug": {
    "enable_debug_logging": false,
    "enable_performance_overlay": false
  }
} 